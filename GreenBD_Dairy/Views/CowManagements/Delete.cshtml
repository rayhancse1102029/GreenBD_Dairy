@using Microsoft.AspNetCore.Http
@model GreenBD_Dairy.Models.CowManagement

@{
    ViewData["Title"] = "Delete";
}


<html>
<head>
    <link href="~/css/Create-Edit-Delete-StyleSheet.css" rel="stylesheet" />
</head>
<body>
    <div class="page-heding">
        <marquee><p>Hello<span style="color: #ffcc00">! </span> <span style="color: #FFCC00">@@</span><span style="color: #FFCC00"> @Context.Session.GetString("username")</span> Are you sure to Delete this Record from Cow Management</p></marquee>
    </div>
    <div class="container">
        <section>
            <div class="create-form">
                <div class="row" id="create-form-id">
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.CowName)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CowName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.PreCodeNo)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.PreCodeNo)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.OurCodeNo)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.OurCodeNo)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.InitialPrice)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.InitialPrice)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CowCollection)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CowCollection.CowCollectionName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CowGroup)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CowGroup.CowGroupName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CowPurpose)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CowPurpose.CowPurposeName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Gender)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Gender.GenderName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.NationOfCow)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.NationOfCow.CowNationName)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Description)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Description)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.ManagerSignature)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.ManagerSignature)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.EntryDate)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.EntryDate)
                        </dd>
                    </dl>
                    <hr />
                    <div>
                        <form asp-action="Delete">
                            <a asp-action="Index" class="btn btn-info">Back to List</a>
                            <input type="hidden" asp-for="Id" />
                            <input type="submit" value="Delete" class="btn btn-danger" />
                        </form>
                    </div>
                    <br />
                </div>
            </div>
        </section>
    </div>
</body>
</html>
