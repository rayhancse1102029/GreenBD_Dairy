    <script src="~/Scroll_Animate/scrollreveal.min.js"></script>


    <script>
        $(document).ready(function () {

            // Scroll Animation


            window.sr = ScrollReveal();

           //   Our Shop Section from here   product-item


            sr.reveal('#product-item-id-one',
                {
                    duration: 2000,
                    origin: 'right',
                    distance: '300px'
                });
            sr.reveal('#product-item-id-two',
                {
                    duration: 2000,
                    origin: 'top',
                    distance: '300px'
                });
            sr.reveal('#product-item-id-three',
                {
                    duration: 2000,
                    origin: 'bottom',
                    distance: '300px'
                });
            sr.reveal('#product-item-id-four',
                {
                    duration: 2000,
                    origin: 'left',
                    distance: '300px'
                });

            // Group Team Member Section from here

            sr.reveal('.team-group',
                {
                    duration: 4000,
                    origin: 'bottom',
                    distance: '300px'
                });
            sr.reveal('#team-member-profile-id-one',
                {
                    duration: 5000,
                    origin: 'left',
                    distance: '300px'
                });
            sr.reveal('#team-member-profile-id-two',
                {
                    duration: 5000,
                    origin: 'top',
                    distance: '300px'
                });
