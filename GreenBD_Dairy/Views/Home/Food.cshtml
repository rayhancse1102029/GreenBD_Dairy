                {
                    duration: 4000,
                    origin: 'bottom',
                    distance: '300px'
                });
            sr.reveal('#blogs-body-items-col-one',
                {
                    duration: 4000,
                    origin: 'left',
                    distance: '300px'
                });
            sr.reveal('#blogs-body-items-col-two',
                {
                    duration: 4000,
                    origin: 'bottom',
                    distance: '300px'
                });
            sr.reveal('#blogs-body-items-col-three',
                {
                    duration: 4000,
                    origin: 'right',
                    distance: '300px'
                });

        });



